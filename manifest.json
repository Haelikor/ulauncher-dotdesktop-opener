{
  "required_api_version": "^2.0.0",
  "name": "DotDesktopOpener",
  "description": "DotDesktop Opener extension",
  "developer_name": "Jose F Rives",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.05
  },
  "preferences": [
    {
      "id": "dotdesktop_kw",
      "type": "keyword",
      "name": "DotDesktop",
      "default_value": "open"
    },
    {
      "id": "dotdesktop_files",
      "type": "text",
      "name": "DotDesktop Files to Open",
      "description": "write the program names separeted by ':' character",
      "default_value": "google-chrome:other"
    }
  ]
}
